<?xml version="1.0"?>

<interfaceDefinition>
  <node name="show">
    <children>
      <node name="dns">
        <children>
          <node name="dynamic">
            <properties>
              <help>Show Dynamic DNS information</help>
            </properties>
            <children>
              <leafNode name="status">
                <properties>
                  <help>Show Dynamic DNS status</help>
                </properties>
                <command>sudo ${vyos_op_scripts_dir}/dynamic_dns_status.py</command>
              </leafNode>
            </children>
          </node>
        </children>
      </node>
    </children>
  </node>
</interfaceDefinition>
